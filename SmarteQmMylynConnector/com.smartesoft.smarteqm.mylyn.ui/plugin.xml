<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="com.smartesoft.smarteqm.mylyn.repository"
         name="%repository.name"
         point="org.eclipse.mylyn.tasks.ui.repositories">
      <connectorCore
            class="com.smartesoft.smarteqm.mylyn.core.internal.SpiraTeamRepositoryConnector"
            id="smarteqm"
            name="%connectorCore.name">
      </connectorCore>
      <connectorUi
            brandingIcon="icons/smarteqm.gif"
            class="com.smartesoft.smarteqm.mylyn.ui.internal.SpiraTeamConnectorUi"
            id="smarteqm"
            name="%connectorUi.name"
            overlayIcon="icons/overlays/smarteqm.gif">
      </connectorUi>
   </extension>
   <extension
         point="org.eclipse.mylyn.tasks.ui.editors">
      <pageFactory
            class="com.smartesoft.smarteqm.mylyn.ui.internal.editor.SpiraTeamTaskEditorFactory"
            id="com.smartesoft.smarteqm.mylyn.ui.pageFactory">
      </pageFactory>
   </extension>

</plugin>
